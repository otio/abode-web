{
  "root": "apps/studio",
  "sourceRoot": "apps/studio/dist",
  "projectType": "application",
  "targets": {
    "dev": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          {
            "command": "sanity start"
          }
        ],
        "cwd": "apps/studio"
      }
    },
    "start": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          {
            "command": "npx sanity/cli start"
          }
        ],
        "cwd": "apps/studio"
      }
    },
    "build": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          {
            "command": "npx sanity/cli"
          }
        ],
        "cwd": "apps/studio"
      }
    }
  }
}
