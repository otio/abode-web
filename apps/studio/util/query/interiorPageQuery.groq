*[ _id == $pageId ]{
    title,
    slug,
    "pageComponents": components[]{
        ...,
         _type == 'bannerContent' => {
          ...,
          bannerImage.asset->{...}
        },
        _type == 'meetTeam' => {
          introText,
          teamImage,
          "page": meetTeamPage{
            ...,
            linkToPage->{...}
          }
        },
        _type == 'reviewPicker' => {
          clientReviews[]->{...}
        },
        _type == 'magazineSignup' => {
          ...,
          promoImage,
          signup->{...}
        },
        _type == 'areasServed' => {
          _key,
          _type,
          areaPages[]->{...}
        },
        // DEPRECATE
        _type == 'marketingCta' => {
          ...,
          ctaCapture->{...}
        },
        _type == 'ctaPicker' => {
          callToActionForms[]->{...}
        },
      }
}