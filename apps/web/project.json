{
  "root": "apps/web",
  "sourceRoot": "apps/web",
  "projectType": "application",
  "targets": {
    "dev": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          {
            "command": "nuxt"
          }
        ],
        "cwd": "apps/web"
      }
    },
    "netlify": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          {
            "command": "netlify dev"
          }
        ],
        "cwd": "apps/web"
      }
    },
    "start": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          {
            "command": "nuxt start"
          }
        ],
        "cwd": "apps/web"
      }
    },
    "build": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          {
            "command": "nuxt generate"
          }
        ],
        "cwd": "apps/web"
      }
    }
  }
}
