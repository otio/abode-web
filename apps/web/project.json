{
  "root": "apps/web",
  "sourceRoot": "apps/web",
  "projectType": "application",
  "targets": {
    "dev": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          {
            "command": "nuxi dev"
          }
        ],
        "cwd": "apps/web"
      }
    },
    "netlify": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          {
            "command": "netlify dev"
          }
        ],
        "cwd": "apps/web"
      }
    },
    "start": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          {
            "command": "nuxi start"
          }
        ],
        "cwd": "apps/web"
      }
    },
    "build": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          {
            "command": "nuxi generate"
          }
        ],
        "cwd": "apps/web"
      }
    },
    "clean": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          {
            "command": "shx rm -rf node_modules/"
          },
          {
            "command": "shx rm -rf .nuxt/"
          },
          {
            "command": "shx rm -rf dist/"
          }
        ],
        "cwd": "apps/web",
        "parallel": false
      }
    }
  }
}
